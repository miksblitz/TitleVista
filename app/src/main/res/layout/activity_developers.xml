<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:id="@+id/roa8qsiywez"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#FFFFFF">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <!-- Back Button -->
            <ImageView
                android:id="@+id/rjsp4hths2d"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="32dp"
                android:layout_marginBottom="13dp"
                android:scaleType="fitXY"
                android:src="@drawable/backbutton" />

            <!-- Title -->
            <TextView
                android:id="@+id/rqg71kne3qi"
                android:textColor="#333333"
                android:textSize="32sp"
                android:layout_marginBottom="53dp"
                android:layout_marginHorizontal="35dp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:gravity="center"
                android:text="Meet the Developers" />

            <!-- Developer 1 -->
            <LinearLayout
                android:id="@+id/rc4xshkqhj6j"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginStart="39dp"
                android:layout_marginBottom="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/rnqi2yvy4ojk"
                    android:layout_width="115dp"
                    android:layout_height="115dp"
                    android:layout_marginEnd="22dp"
                    android:src="@drawable/monleonpic"
                    android:scaleType="fitXY" />

                <LinearLayout
                    android:id="@+id/repgyadls98n"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/rb8eytf6tmmr"
                        android:textColor="#441D83"
                        android:textSize="20sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Lliam Khenzo Monleon" />

                    <TextView
                        android:id="@+id/rcg6f2bu0dt"
                        android:textColor="#000000"
                        android:textSize="12sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Developer" />
                </LinearLayout>
            </LinearLayout>

            <!-- Developer 2 -->
            <LinearLayout
                android:id="@+id/ram6mf86z34"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginStart="39dp"
                android:layout_marginBottom="30dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/rtlg0eh83qb"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="22dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/rbw1mm8utiiq"
                        android:textColor="#441D83"
                        android:textSize="20sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Mikel Robbie Aboyme" />

                    <TextView
                        android:id="@+id/rq1fmj3tepxe"
                        android:textColor="#000000"
                        android:textSize="12sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Creative designer" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/rn7f5apoljim"
                    android:layout_width="115dp"
                    android:layout_height="115dp"
                    android:src="@drawable/aboymepic"
                    android:scaleType="fitXY" />
            </LinearLayout>

            <!-- Developer 3 -->
            <LinearLayout
                android:id="@+id/rzj3apkukgr"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginStart="39dp"
                android:layout_marginBottom="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/r08jegrz5ychc"
                    android:layout_width="115dp"
                    android:layout_height="115dp"
                    android:layout_marginEnd="22dp"
                    android:src="@drawable/doriapic"
                    android:scaleType="fitXY" />

                <LinearLayout
                    android:id="@+id/rvq6wjv7n3k"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/ruaxmxqnhpr"
                        android:textColor="#441D83"
                        android:textSize="20sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Ma. Tricia Alexandra Doria" />

                    <TextView
                        android:id="@+id/r33yczwypjj9"
                        android:textColor="#000000"
                        android:textSize="12sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="UI/UX Designer" />
                </LinearLayout>
            </LinearLayout>

            <!-- Developer 4 -->
            <LinearLayout
                android:id="@+id/rn2gwluh4cse"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginStart="39dp"
                android:layout_marginBottom="50dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/razanyqj3trd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="22dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/rf3rcsvjkspq"
                        android:textColor="#441D83"
                        android:textSize="20sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Hendria Masiado" />

                    <TextView
                        android:id="@+id/rtyffskr7l7"
                        android:textColor="#000000"
                        android:textSize="12sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Marketer" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/rmj2i1eyouoh"
                    android:layout_width="115dp"
                    android:layout_height="115dp"
                    android:src="@drawable/masiadopic"
                    android:scaleType="fitXY" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>